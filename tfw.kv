<LoginPage>:
    canvas.before:
        Rectangle:
            pos: self.pos
            size: self.size
            source: 'bg-2.png'
    name: "login_page"
    FloatLayout:
        canvas.before:
            Rectangle:
                pos: self.center_x -200, self.center_y
                size: (400,400)
                source: 'logo.png'
        Label:
            id: info
            text: ''
            markup: True
            pos_hint: {"x":0,"top": 0.98}

        TextInput:
            id: login
            hint_text: "Username.."
            font_size: 50
            multiline: False
            halign: 'left'
            valign: 'middle'
            write_tab: False

            pos_hint: {"x":0.25,"top": 0.45}
            size_hint: 0.5,0.05
            #size_hint_y:None
        CheckBox:
            id: rem_pass
            disabled:True
            size_hint: 0.07,0.07
            pos_hint: {"x":0.77,"top": 0.35}
        CheckBox:
            id: rem_user
            on_active: root.if_active(self.active)
            size_hint: 0.07,0.07
            pos_hint: {"x":0.77,"top": 0.45}

        TextInput:
            id: passw

            hint_text: "Password.."
            font_size: 50
            multiline: False
            halign: 'left'
            write_tab: False
            valign: 'middle'
            size_hint: 0.5,0.05
            pos_hint: {"x":0.25,"top": 0.35}
            password: True # hide password



        Button:
            id: btn2
            text: "Login"
            font_size: 36
            size_hint: 0.3,0.07
            pos_hint: {"x":0.35,"top": 0.25}
            on_release:
                app.root.transition = WipeTransition()
                root.verify_credentials()





